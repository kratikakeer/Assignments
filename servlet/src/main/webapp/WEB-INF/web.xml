<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<welcome-file-list>
	<welcome-file> index.jsp </welcome-file>
    </welcome-file-list>
    
    <servlet>
      <servlet-name>bhushan</servlet-name>
      <servlet-class>com.te.servlet.ToMaharaj</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>bhushan</servlet-name>
      <url-pattern>/maharaj</url-pattern>
    </servlet-mapping>
    
    <servlet>
      <servlet-name>login</servlet-name>
      <servlet-class>com.te.login.Login</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>login</servlet-name>
      <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>addition</servlet-name>
        <servlet-class>com.te.addition.Addition</servlet-class>
     </servlet>
     <servlet-mapping>
         <servlet-name>addition</servlet-name>
         <url-pattern>/addition</url-pattern>
     </servlet-mapping>
     
     <servlet>
     <servlet-name>subtraction</servlet-name>
     <servlet-class>com.te.httpservlet.Subtraction</servlet-class>
     </servlet>
     <servlet-mapping>
     <servlet-name>subtraction</servlet-name>
     <url-pattern>/subtraction</url-pattern>
     </servlet-mapping>
     
     <servlet>
      <servlet-name>loginpage</servlet-name>
      <servlet-class>com.te.loginpage.LoginPage</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>loginpage</servlet-name>
      <url-pattern>/welcomepage</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.te.loginpage.SignUp</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
    <servlet-name>validate</servlet-name>
    <servlet-class>com.te.loginpage.Validate</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>validate</servlet-name>
    <url-pattern>/validate</url-pattern>
    </servlet-mapping>
    


</web-app>